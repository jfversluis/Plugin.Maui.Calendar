<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Plugin.Maui.CalendarStore.Sample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:Plugin.Maui.CalendarStore.Sample"
    Shell.FlyoutBehavior="Disabled">

	<TabBar>
		<ShellContent
			Title="Calendars"
			ContentTemplate="{DataTemplate pages:CalendarsPage}" />

		<ShellContent
			Title="Events"
			ContentTemplate="{DataTemplate pages:EventsPage}" />
	</TabBar>
</Shell>
